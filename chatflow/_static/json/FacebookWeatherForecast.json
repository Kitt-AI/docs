        "func": "var data=msg.payload\n\nvar state=data.weather[0].main;\nvar icon='http://openweathermap.org/img/w/'+data.weather[0].icon+'.png';\nvar temperatrure=data.main.temp;\nvar pressure=data.main.pressure;\nvar humidity=data.main.humidity;\nvar windSpeed=data.wind.speed;\nvar windDegree=data.wind.deg;\n\nvar getWindDirectionByDegree = function (degree) {\n    var directions = ['NNE', 'NE', 'ENE', 'E', 'ESE', 'SE', 'SSE', 'S', 'SSW',\n        'SW', 'WSW', 'W', 'WNW', 'NW', 'NNW', 'N'];\n\n    for (var i = 0; i < directions.length; i++) {\n        degree -= 22.5;\n        if (degree < 0)\n            return directions[i];\n    }\n\n    return directions[i];\n}\n\nvar windDirection=getWindDirectionByDegree(windDegree);\n\nvar buttons=[\n{\n    \"type\":\"postback\",\n    \"title\":\"Forecast\",\n    \"payload\":\"forecast\"\n},\n{\n    \"type\":\"web_url\",\n    \"title\":\"Bot created at ChatFlow\",\n    \"url\":\"https://chatflow.kitt.ai\"\n}]\n\nmsg.facebook.attachment={\n  \"type\":\"template\",\n  \"payload\":{\n    \"template_type\":\"generic\",\n    \"elements\":[\n      {\n        \"title\":\"Today's weather at \"+msg.city+\":\",\n        \"image_url\":icon,\n        \"subtitle\":state,\n        \"buttons\":buttons\n      },\n      {\n          \"title\":\"Temperature:\",\n          \"subtitle\":temperatrure+\" Â°C\",\n          \"buttons\":buttons\n      },\n      {\n          \"title\":\"Wind:\",\n          \"subtitle\":windSpeed+\" m/s \"+windDirection,\n          \"buttons\":buttons\n      },\n      {\n          \"title\":\"pressure:\",\n          \"subtitle\":pressure+\" hpa\",\n          \"buttons\":buttons\n      },\n      {\n          \"title\":\"Humidity:\",\n          \"subtitle\":humidity+\" %\",\n          \"buttons\":buttons\n      }\n    ]\n  }\n}\n\nmsg.payload=null;\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "requestUserInput": false,
        "finalstate": false,
        "response": ""
    },
